  "main": "dist/main.js",
  "scripts": {
    "build:dev": "yarn run clean && webpack --colors --mode development --progress",
    "build:prod": "yarn run clean && webpack --colors --mode production --progress",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "yarn run build:dev && cross-env NODE_ENV=development node dist/main.js",
    "start:prod": "yarn run build:prod && cross-env NODE_ENV=production node dist/main.js",
    "watch": "nodemon src/index.js --exec babel-node"
  },
  "dependencies": {
    "apollo-errors": "^1.7.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express-graphql": "^0.6.12",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.23.1",
    "helmet": "^3.12.0",
    "mongoose": "^5.0.12",
    "morgan": "^1.9.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.17.2",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  }
