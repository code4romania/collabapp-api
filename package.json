{
  "name": "collab-api",
  "version": "0.0.1",
  "author": {
    "name": "Code4Romania",
    "email": "code4romania@code.ro"
  },
  "main": "dist/main.js",
  "scripts": {
    "build:dev": "yarn run clean && webpack --colors --mode development --progress",
    "build:prod": "yarn run clean && webpack --colors --mode production --progress",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "yarn run build:dev && cross-env NODE_ENV=development node dist/main.js",
    "start:prod": "yarn run build:prod && cross-env NODE_ENV=production node dist/main.js",
    "watch": "nodemon src/index.js --exec babel-node"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "morgan": "^1.9.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "nodemon": "^1.15.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-node-externals": "^1.6.0"
  }
}
